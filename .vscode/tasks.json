{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build STM32 project with Libraries",
			"type": "shell",
			"command": "make -f STM32MakeWithLibs.make",
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"label": "Build and flash to an STM32 platform with Libraries",
			"dependsOn": "Build STM32 project with Libraries",
			"type": "process",
			"command": "${command:stm32-for-vscode.flash}",
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"label": "Flash STM",
			"type": "process",
			"command": "${command:stm32-for-vscode.flash}",
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"label": "Build STM",
			"type": "process",
			"command": "${command:stm32-for-vscode.build}",
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"label": "Build Clean STM",
			"type": "process",
			"command": "${command:stm32-for-vscode.cleanBuild}",
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$gcc"
			]
		}
	]
}